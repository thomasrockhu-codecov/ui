{
  "name": "@nordnet/ui",
  "version": "0.0.0",
  "author": "Nordnet <frontend@nordnet.se> (https://nordnet.se)",
  "repository": "https://github.com/nordnet/ui",
  "bugs": "https://github.com/nordnet/ui/issues",
  "main": "dist/cjs",
  "module": "dist/esm",
  "types": "dist/types",
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "TZ=UTC jest",
    "tdd": "npm run test -- --watch",
    "test-ci": "TZ=UTC jest --coverage --ci && codecov",
    "dev": "sh scripts/start-storybook.sh",
    "lint:js": "eslint --ext=.tsx --ext=.ts src",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint": "yarn run lint:js && yarn run lint:css && tsc",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "babel:esm": "NODE_ENV=production BABEL_ENV=esm babel -d dist/esm src/ --extensions=.ts,.tsx --copy-files",
    "babel:cjs": "NODE_ENV=production babel -d dist/cjs src/ --extensions=.ts,.tsx --copy-files",
    "build": "rimraf dist && yarn babel:esm && yarn babel:cjs && tsc && rimraf dist/**/*.types.js dist/**/*.d.js dist/**/*.stories.js",
    "build-storybook": "build-storybook -s ./static",
    "now-build": "build-storybook",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "prepublishOnly": "yarn build",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-case": [
        0
      ],
      "header-max-length": [
        0
      ]
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@popperjs/core": "^2.9.3",
    "@xstate/react": "^0.7.1",
    "color": "^4.0.1",
    "date-fns": "^2.23.0",
    "focus-within-polyfill": "^5.2.1",
    "framer-motion": "^1.9.1",
    "ramda": "^0.27.1",
    "react-focus-lock": "^2.5.2",
    "react-intl": "^5.20.10",
    "react-popper": "^2.2.5",
    "react-remove-scroll": "^2.4.3",
    "react-uid": "^2.3.1",
    "scroll-into-view-if-needed": "^2.2.28",
    "styled-components": "5.1.1",
    "use-ssr": "^1.0.24",
    "xstate": "^4.23.1"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.15.0",
    "@babel/helper-validator-identifier": "^7.14.9",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "@commitlint/travis-cli": "7.6.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@storybook/addon-a11y": "6.3.7",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-knobs": "6.3.0",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-storyshots": "^6.3.7",
    "@storybook/addon-storysource": "6.3.7",
    "@storybook/addon-viewport": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@storybook/source-loader": "^6.3.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/color": "3.0.2",
    "@types/jest": "27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.7.1",
    "@types/ramda": "0.27.44",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "@types/styled-components": "5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "babel-loader": "8.2.2",
    "babel-plugin-ramda": "2.0.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-styled-components": "1.13.2",
    "babel-plugin-transform-remove-imports": "1.5.5",
    "babel-plugin-typescript-to-proptypes": "1.4.2",
    "babel-preset-react-app": "^10.0.0",
    "codecov": "3.8.3",
    "csstype": "^3.0.8",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "git-cz": "3.0.1",
    "husky": "^7.0.2",
    "jest": "27.0.6",
    "jest-date-mock": "^1.0.8",
    "jest-specific-snapshot": "^5.0.0",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "11.1.2",
    "lodash": "^4.17.21",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-live": "^2.3.0",
    "react-markdown": "4.0.8",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "16.0.0-beta.18",
    "storybook-addon-intl": "2.4.1",
    "storybook-dark-mode": "^1.0.8",
    "stylelint": "10.0.1",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.3.5",
    "webpack": "4.32.0"
  },
  "resolutions": {
    "websocket-extensions": "^0.1.4"
  }
}
